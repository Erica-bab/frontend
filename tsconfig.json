{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.cjs"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
  ],
  "extends": "expo/tsconfig.base"
}
